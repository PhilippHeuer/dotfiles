        "interval": 2,
        "state-icons": {
            "paused": "ÔÅå",
            "playing": "ÔÅã"
        },
        "on-click": "mpc toggle",
        "tooltip-format": "MPD (connected)",
        "tooltip-format-disconnected": "MPD (disconnected)"
    },
    "mpris": {
        "format": "{player_icon} {dynamic}",
        "format-paused": "{status_icon} <i>{dynamic}</i>",
        "player-icons": {
            "spotify": "ÔÜº",
            "default": "üéµ"
        },
        "dynamic-order": [
            "title"
        ],
        "interval": 5,
        "status-icons": {
            "paused": "‚è∏"
        },
        "ignored-players": [
            "firefox"
        ]
    },

    // Separators
    "custom/separator#dot": {
        "format": "ÔëÑ",
        "interval": "once",
        "tooltip": false
    },
    "custom/separator#dot-line": {
        "format": "Óò°",
        "interval": "once",
        "tooltip": false
    },
    "custom/separator#line": {
        "format": "|",
